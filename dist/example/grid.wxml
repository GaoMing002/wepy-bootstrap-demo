            <view class="_div wbd-example-cd card border-0">
                <view class="_div card-body">
                    <view class="_div bg-light lead rounded-top  p-2">混合宽度组合</view>

                    <view class="_div container">
                        <!-- Stack the columns on mobile by making one full-width and the other half-width -->
                        <view class="_div row">
                            <view class="_div col-8">.col-8</view>
                            <view class="_div col-4">.col-4</view>
                        </view>

                        <!-- Columns start at 50% wide on mobile and bump up to 33.3% wide on desktop -->
                        <view class="_div row">
                            <view class="_div col-4">.col-4</view>
                            <view class="_div col-4">.col-4</view>
                            <view class="_div col-4"> .col-4</view>
                        </view>

                        <!-- Columns are always 50% wide, on mobile and desktop -->
                        <view class="_div row">
                            <view class="_div col-6">.col-6</view>
                            <view class="_div col-6">.col-6</view>
                        </view>
                    </view>

                </view>
            </view>

