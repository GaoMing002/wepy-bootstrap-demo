._li{
  display: list-item; }
._ol{
  list-style-type: decimal;
  padding-left: 1.15rpx; }
._ul ._ul{
  list-style-type: circle; }
._ul{
  list-style-type: disc;
  padding-left: 1.15rpx; }
text,
    text::before,
    text::after{
  box-sizing: border-box; }
view,
    view::before,
    view::after{
  box-sizing: border-box; }
